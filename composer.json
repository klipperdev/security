{
    "name": "klipper/security",
    "type": "library",
    "description": "Security component of Klipper",
    "keywords": ["RBAC", "E-RBAC", "Permission", "Sharing", "Security"],
    "homepage": "https://github.com/klipperdev/security",
    "license": "MIT",
    "authors": [
        {
            "name": "Fran√ßois Pluchino",
            "email": "francois.pluchino@klipper.dev"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4.0|^8.0.0",
        "doctrine/collections": "^1.5.0",
        "doctrine/event-manager": "^1.0.0",
        "doctrine/persistence": "^2.0.0",
        "klipper/config": "^2.0.0",
        "klipper/doctrine-extra": "^2.0.0",
        "psr/cache": "^1.0.0",
        "symfony/event-dispatcher": "^5.2.0",
        "symfony/expression-language": "^5.2.0",
        "symfony/http-kernel": "^5.2.0",
        "symfony/property-access": "^5.2.0",
        "symfony/security-core": "^5.2.0",
        "symfony/validator": "^5.2.0"
    },
    "require-dev": {
        "doctrine/annotations": "^1.6.0",
        "doctrine/orm": "^2.8.0",
        "klipper/cache": "^2.0.0",
        "klipper/doctrine-extensions": "^2.0.0",
        "symfony/config": "^5.2.0",
        "symfony/dependency-injection": "^5.2.0",
        "symfony/finder": "^5.2.0"
